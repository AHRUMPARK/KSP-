function youtubeDataMove() {
    let temp = [];
    let obj = {};
    let title, rank, lastWeekRank, albumImg, singer, chartDuration, views, link;
    '<% for(let i = 0; i < youtubedata.data.length; i++) { %>'
        title = '<%= youtubedata.data[i].title %>';
        rank = '<%= youtubedata.data[i].rank %>';
        lastWeekRank = '<%= youtubedata.data[i].lastWeekRank %>';
        albumImg = '<%= youtubedata.data[i].albumImg %>';
        singer = '<%= youtubedata.data[i].singer %>';
        chartDuration = '<%= youtubedata.data[i].chartDuration %>';
        views = '<%= youtubedata.data[i].views %>';
        link = '<%= youtubedata.data[i].link %>';

        obj = {
            "title": title,
            "rank": rank,
            "lastWeekRank": lastWeekRank,
            "albumImg": albumImg,
            "singer": singer,
            "chartDuration": chartDuration,
            "views": views,
            "link": link,
        }
        temp.push(obj);
    '<% } %>'

    // console.log(temp);
    return temp;
}

